#, c-format
msgid "-Z        Remove all packages from the \"keep\" file.\n"
msgstr "-Z        удалить все пакеты из файла «keep»\n"

#: src/exit.c:125
#, c-format
msgid "--df-keep                   Read debfoster's \"keepers\" file.\n"
msgstr "--df-keep                   читать файл «keepers» от debfoster\n"

